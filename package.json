    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.16.0",
    "parcel": "^2.16.0"
